<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="horizontal" android:id="@+id/edco_buttonlayout" android:layout_width="match_parent" android:layout_alignParentBottom="true" android:layout_height="40dip">
        <Button android:layout_weight="1" android:layout_width="match_parent" android:text="Button" android:layout_height="match_parent" android:id="@+id/edco_ok"></Button>
        <Button android:layout_weight="1" android:layout_width="match_parent" android:text="Button" android:layout_height="match_parent" android:id="@+id/edco_cancel"></Button>
    </LinearLayout>
    <LinearLayout android:id="@+id/edco_layout" android:layout_width="match_parent" android:orientation="vertical" android:layout_height="match_parent" android:layout_above="@+id/edco_buttonlayout">
        <TextView android:id="@+id/edco_cachename" android:text="TextView" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
        <Button android:id="@+id/edco_buttoncoord" android:layout_width="match_parent" android:text="Button" android:layout_height="wrap_content"></Button>
        <TextView android:id="@+id/edco_typtext" android:text="TextView" android:layout_width="match_parent" android:layout_height="wrap_content"></TextView>
        <Spinner android:layout_width="match_parent" android:id="@+id/edco_typspinner" android:layout_height="wrap_content"></Spinner>
        <TextView android:id="@+id/edco_titletext" android:text="TextView" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
        <EditText android:text="EditText" android:id="@+id/edco_titleedit" android:layout_width="match_parent" android:layout_height="wrap_content"></EditText>
    </LinearLayout>
</RelativeLayout>
