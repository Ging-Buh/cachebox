    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="1874872" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Zeppelinplatz</groundspeak:name>
      <groundspeak:placed_by>Luksar</groundspeak:placed_by>
      <groundspeak:owner id="2568249">Luksar</groundspeak:owner>
