    <groundspeak:cache id="293018" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Pankemühle</groundspeak:name>
      <groundspeak:placed_by>CyberMan54</groundspeak:placed_by>
