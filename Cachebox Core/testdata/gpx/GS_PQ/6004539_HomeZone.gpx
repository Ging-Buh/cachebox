    <desc>Sprengelkiez by museion, Multi-cache (1.5/1.5)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=ed40da6b-0215-447b-a6d2-f93aa9389980</url>
    <urlname>Sprengelkiez</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="801312" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Sprengelkiez</groundspeak:name>
      <groundspeak:placed_by>museion</groundspeak:placed_by>
      <groundspeak:owner id="1356631">museion</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
      <groundspeak:attributes>
        <groundspeak:attribute id="33" inc="1">Motorcycles</groundspeak:attribute>
        <groundspeak:attribute id="30" inc="1">Picnic tables nearby</groundspeak:attribute>
        <groundspeak:attribute id="13" inc="1">Available at all times</groundspeak:attribute>
