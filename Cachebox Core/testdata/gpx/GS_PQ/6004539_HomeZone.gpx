    <groundspeak:cache id="1895029" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Scrabble-Seltsamkeiten</groundspeak:name>
      <groundspeak:placed_by>monsta13</groundspeak:placed_by>
      <groundspeak:owner id="1272444">monsta13</groundspeak:owner>
