      <groundspeak:name>FlakGefecht</groundspeak:name>
      <groundspeak:placed_by>moha</groundspeak:placed_by>
      <groundspeak:owner id="227816">moha</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
