    <groundspeak:cache id="2122754" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Isotania 2 (NC/UV)</groundspeak:name>
      <groundspeak:placed_by>NSK74</groundspeak:placed_by>
