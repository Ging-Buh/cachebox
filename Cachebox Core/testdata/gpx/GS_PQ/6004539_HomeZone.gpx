    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=45af5424-feb1-4281-a2ab-92f788395a56</url>
    <urlname>Stettiner Bahnhof</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="1288849" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Stettiner Bahnhof</groundspeak:name>
      <groundspeak:placed_by>Kallieber</groundspeak:placed_by>
      <groundspeak:owner id="1529963">kallieber</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
