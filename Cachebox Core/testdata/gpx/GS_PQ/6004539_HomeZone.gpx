  <wpt lat="52.567267" lon="13.41225">
    <time>2009-06-09T07:00:00Z</time>
    <name>GC1TG0K</name>
    <desc>Die Legende by mikrofuchs, Unknown Cache (2.5/1)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=d21d4022-e30a-442e-8880-96f47855d454</url>
    <urlname>Die Legende</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Unknown Cache</type>
    <groundspeak:cache id="1272571" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Die Legende</groundspeak:name>
      <groundspeak:placed_by>mikrofuchs</groundspeak:placed_by>
      <groundspeak:owner id="1454733">mikrofuchs</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Not chosen</groundspeak:container>
      <groundspeak:attributes>
        <groundspeak:attribute id="32" inc="1">Bicycles</groundspeak:attribute>
        <groundspeak:attribute id="26" inc="1">Public transportation</groundspeak:attribute>
        <groundspeak:attribute id="25" inc="1">Parking available</groundspeak:attribute>
        <groundspeak:attribute id="1" inc="1">Dogs</groundspeak:attribute>
        <groundspeak:attribute id="33" inc="1">Motorcycles</groundspeak:attribute>
      </groundspeak:attributes>
      <groundspeak:difficulty>2.5</groundspeak:difficulty>
