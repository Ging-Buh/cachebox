    <groundspeak:cache id="2124522" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Das große Krabbeln</groundspeak:name>
      <groundspeak:placed_by>Bratapfel</groundspeak:placed_by>
      <groundspeak:owner id="2899777">Bratapfel</groundspeak:owner>
