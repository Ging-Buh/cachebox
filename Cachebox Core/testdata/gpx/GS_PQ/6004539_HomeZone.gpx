      <groundspeak:long_description html="True">&lt;div style="text-align: center;"&gt;
&lt;big style="font-family: Adobe Garamond Pro Bold;"&gt;&lt;big&gt;&lt;big&gt;&lt;big&gt;&lt;big&gt;
&lt;small style="color: rgb(0, 0, 153); font-family: Arno Pro Light Display; font-weight: bold;"&gt;
