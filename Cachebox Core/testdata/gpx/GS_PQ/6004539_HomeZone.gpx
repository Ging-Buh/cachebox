    <groundspeak:cache id="2028671" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Hamburger Bahnhof</groundspeak:name>
      <groundspeak:placed_by>Kallieber</groundspeak:placed_by>
