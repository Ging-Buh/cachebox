    <groundspeak:cache id="650270" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Oasen der Besinnung III</groundspeak:name>
      <groundspeak:placed_by>Taqman</groundspeak:placed_by>
      <groundspeak:owner id="938088">TaqmanOne</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
      <groundspeak:attributes>
