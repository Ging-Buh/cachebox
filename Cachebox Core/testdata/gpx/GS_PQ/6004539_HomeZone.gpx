    <groundspeak:cache id="1317657" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Märchenland</groundspeak:name>
      <groundspeak:placed_by>ElliBi</groundspeak:placed_by>
      <groundspeak:owner id="2260390">ElliBi</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
