    <type>Geocache|Earthcache</type>
    <groundspeak:cache id="1282543" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>„Großer Stein“ von Buchholz</groundspeak:name>
      <groundspeak:placed_by>Mr.Pete</groundspeak:placed_by>
      <groundspeak:owner id="861802">Mr.Pete</groundspeak:owner>
      <groundspeak:type>Earthcache</groundspeak:type>
      <groundspeak:container>Not chosen</groundspeak:container>
