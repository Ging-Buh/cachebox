  <!-- -->
  <!-- Anmerkungen: -->
  <!-- + Die Zeichenreihenfolge der Objekte ergibt sich aus: -->
  <!--   + 1. dem Wert des OSM-Tags "layer" (-5 ... 0 ... +5) -->
  <!--   + 2. der Regelreihenfolge in diesem Theme -->
  <!-- + Die OSM-Objekte werden "umfangreich" durch das Tool "fzk_osmfilter" modifiziert. -->
  <!-- + D.h. dieses Theme kann generell nur mit entsprechenden Karte benutzt werden. -->
  <!-- + Dito koennen keine "anderen" Themes mit den Freizeitkarten benutzt werden. -->
  <!-- -->
  <!-- Verwendete Polygone (Reihenfolge beachten): -->
  <!-- Logische Gewichtung: Liste mit Objekten -->
  <!-- ======================================= -->
  <!-- 1: Hintergrund (wird global festgelegt) -->
  <!-- 2: Meer (wird global festgelegt) -->
  <!-- 3: Land, Weinberg, Schlick -->
  <!-- 4: Wohngebiet, Schule, Handel, Krankenhaus, Steinbruch, Golfplatz, Industrie, Raststaette, Schrebergaerten, Kulturland (Acker, Wiese), Alm/Bergwiese, Verwaltung -->
  <!-- 5: Laubwald, Park, Deponie, Heide, Geroell, Nadelwald, Mischwald, Gehoeft -->
  <!-- 6: Fels, Wiese (innerorts), Sportanlage, Zoo, Kraftwerk, Baugebiet, Flugplatz, Strand, verkehrsberuhiger Bereich, Platz, Fussgaengerzone, Station, Sand, Gletscher, Plantage, Gebuesch -->
  <!-- 7: Vorfeld, Spielplatz, Camping, Friedhof, Kasernen, Andachtsstaette, Spielfeld/Laufbahn, Terminal, Wasser, Bahnsteig, Wasser, Spielfeld, Flussufer, Watt, Parkplatz -->
  <!-- 8: Sumpf, Grasland, Dock, Gefahrenzone, Yachthafen, Anlegestelle, Militaer -->
  <!-- 9: Gebaeude, Staudamm/-mauer, Buhne, Naturschutzgebiet -->
  <!-- -->
  <!-- Konzeptionelle Anmerkungen zum Thema "Flaechen": -->
  <!-- Ein Weg-Objekttyp kann auftreten als Weg (way), ringfoermiger Weg (closedway) oder als Flaeche (closedway). -->
  <!-- Bei Objekten die als "way" und als "closedway" auftreten koennen, wird typischerweise -->
  <!-- zusaetzlich das Tag "area=yes" gesetzt, um das Objekt explizit als Flaeche auszuweisen. -->
  <!-- Fuer die nachfolgenden Flaechenobjekte wird grundsaetzlich das Flag "closed=yes" erwartet. -->
  <!-- Dies auch dann, wenn das Tag (z.B. landuse=residential) gemaess Definition nur ein "closedway" sein darf. -->
  <!-- -->
  <!-- Flaechen die sich hinsichtlich ihrer Groesse signifikant unterscheiden koennen, werden differenziert betrachtet. -->
  <!-- Das Bewertungskriterium hierfuer ist die Groesse der Flaeche des umgebenden Rechtecks (bounding-Box). -->
  <!-- Dieses wird durch die Eckpunkte "Sued-West" (unten links) und "Nord-Ost" (oben rechts) bestimmt. -->
  <!-- Die Quadratmetergroesse, als Logarithums zur Basis 2 (log2(Quadartmeter)), dient dann zur Bewertung. -->
  <!-- Es gilt (Beispiel fuer "forest"): -->
  <!-- + landuse=forest >= 26 = forest_huge   (>= 67.108.864 qm) -->
  <!-- + landuse=forest >= 24 = forest_large  (>= 16.777.216 qm) -->
  <!-- + landuse=forest >= 22 = forest_medium (>=  4.194.304 qm) -->
  <!-- + landuse=forest >= 20 = forest_small  (>=  1.048.576 qm) -->
  <!-- + landuse=forest >= 18 = forest_tiny   (>=    262.144 qm) -->
  <!-- + landuse=forest >= 16 = forest_micro  (>=     65.536 qm) -->
  <!-- + landuse=forest >= 14 = forest_nano   (>=     16.384 qm) -->
  <!-- + landuse=forest <  14 = forest        (<      16.384 qm) -->
  <!-- -->
  <!-- Hinweis auf weitere Unterlagen: -->
  <!-- + siehe auch freizeitkarte.xml -->
  <!-- + siehe auch fzk_osmfilter.c, fzk_globals.c, fzk_nodes.c, fzk_ways.c, fzk_relations.c -->
  <!-- -->
  <!-- Ueber Marker und die App "Theme-Configurator" koennen Eigenschaften dynamisch veraendert werden. -->
  <!-- Alle Marker werden im Theme definiert - die App selbst ist generisch. -->
  <!-- Separatoren: -->
  <!-- @@@ = definiert Kategorie -->
  <!-- ### = definiert Konfigurationsoption -->
  <!-- === = (linke Seite = ISO-Sprachcode 639-3); (rechte Seite = Sprachtext) -->
  <!-- ||| = trennt mehrere Sprachtexte voneinander -->
  <!-- -->
<!--BEGINN_THEME_CONFIGURATOR
@@@FLAECHEN|||deu===Flächen|||eng===Areas
@@@LINIEN|||deu===Linien|||eng===Lines
@@@POINTS|||deu===Punkte|||eng===Points
###ACKERLAND@@@FLAECHEN|||deu===Ackerland|||eng===Farmland
###NATURSCHUTZGEBIET@@@FLAECHEN|||deu===Naturschutzgebiete|||eng===Nature reserves
###BUNDESLANDGRENZE@@@LINIEN|||deu===Bundeslandgrenzen (4)|||eng===Federal states borders (4)
###GEMEINDEGRENZE@@@LINIEN|||deu===Gemeindegrenzen (7,8)|||eng===City-districts (7,8)
###HOEHENLINIE@@@LINIEN|||deu===Höhenlinien|||eng===Contours
###KREISGRENZE@@@LINIEN|||deu===Kreisgrenzen (6)|||eng===County borders (6)
###REGIERUNGSBEZIRKSGRENZE|||LINIEN###deu===Regierungsbezirksgrenzen (5)|||eng===State-district borders (5)
###STAATSGRENZE@@@LINIEN|||deu===Staatsgrenzen (2)|||eng===National borders (2)
###UEBERLANDLEITUNG@@@LINIEN|||deu===Überlandleitungen|||eng===Transmission lines
###ANBETUNGSORT@@@PUNKTE|||deu===Anbetungsorte|||eng===Place of worship
###BUS@@@PUNKTE|||deu===Bus|||eng===Bus
###GELDAUTOMAT@@@PUNKTE|||deu===Geldautomat|||eng===Atm
###STRASSENBAHN@@@PUNKTE|||deu===Straßenbahn|||eng===Tram
###U-BAHN@@@PUNKTE|||deu===U-Bahn|||eng===Subway
###UNTERKUNFT@@@PUNKTE|||deu===Unterkunft|||eng===Accommodation
###INFORMATION@@@PUNKTE|||deu===Information|||eng===Information
###BAUM@@@PUNKTE|||deu===Baum|||eng===Tree
###SCHLAGBAUM@@@PUNKTE|||deu===Schlagbaum|||eng===Liftgate
###TOR@@@PUNKTE|||deu===Tor|||eng===Gate
###MUSEUM@@@PUNKTE|||deu===Museum|||eng===Museum
###AUSSICHTSPUNKT@@@PUNKTE|||deu===Aussichtspunkt|||eng===Viewpoint
ENDE_THEME_CONFIGURATOR-->
  <!-- -->
  <!-- -->
  <!-- -->
  <!-- CLOSED WAYS -->
  <!-- =========== -->
  <!-- -->
  <rule e="way" k="*" v="*">
    <!-- -->
    <!-- +++ Wohngebiet +++ -->
    <!-- Wohngebiet. -->
    <rule e="way" k="landuse" v="residential_huge|residential_large|residential_medium|residential_small|residential_tiny|residential_micro|residential_nano|residential" closed="yes">
      <area fill="#dddddd"/>
    </rule>
  </rule>
 
  <!-- ============================================================ -->
  <!-- -->
  <!-- WAYS -->
  <!-- ==== -->
  <!-- -->
  <rule e="way" k="*" v="*">
    <!-- -->
    <!-- -->
    <!-- +++ Nutzungseinschraenkungen auf Strassen und Wegen +++ -->
    <!-- -->
    <!-- Folgende Wege unterliegen generell keiner Nutzungseinschraenkung: -->
    <!-- + motorway, trunk, primary, secondary, tertiary -->
    <!-- -->
    <!-- Nutzungseinschraenkungen auf allgemeinen Strassen -->
    <rule e="way" k="highway" v="residential|unclassified|road|service" zoom-min="15">
      <!-- gesperrt = rosa transparentes Overlay (gestrichelt oder durchgehend) -->
      <rule e="way" k="access" v="no|private|agricultural|forestry">
        <rule e="way" k="foot" v="yes">
          <!-- kein Kraftverkehr; Fussgaenger (und meist auch Radfahrer) sind erlaubt (gestricheltes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="0.6" stroke-dasharray="10,15" stroke-linecap="round"/>
        </rule>
        <rule e="way" k="foot" v="no">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="0.6" stroke-linecap="round"/>
        </rule>
        <rule e="way" k="foot" v="~">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="0.6" stroke-linecap="round"/>
        </rule>
      </rule>
      <!-- eingeschraenkt = gruen transparentes Overlay -->
      <rule e="way" k="access" v="customers|delivery|destination|permissive">
        <line stroke="#8880FF00" stroke-width="0.6" stroke-dasharray="10,15" stroke-linecap="round"/>
      </rule>
    </rule>
    <!-- -->
    <!-- Nutzungseinschraenkungen auf Wohnstrassen und "guten" Feldwegen -->
    <rule e="way" k="highway" v="living_street|feldweg1" zoom-min="15">
      <!-- gesperrt = rosa transparentes Overlay -->
      <rule e="way" k="access" v="no|private|agricultural|forestry">
        <rule e="way" k="foot" v="yes">
          <!-- kein Kraftverkehr; Fussgaenger sind erlaubt (gestricheltes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="1.8" stroke-dasharray="10,15" stroke-linecap="round"/>
        </rule>
        <rule e="way" k="foot" v="no">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="1.8" stroke-linecap="round"/>
        </rule>
        <rule e="way" k="foot" v="~">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#66FF6FCF" stroke-width="1.8" stroke-linecap="round"/>
        </rule>
      </rule>
      <!-- eingeschraenkt = gruen transparentes Overlay -->
      <rule e="way" k="access" v="customers|delivery|destination|permissive">
        <line stroke="#8880FF00" stroke-width="1.8" stroke-dasharray="10,15" stroke-linecap="round"/>
      </rule>
    </rule>
    <!-- -->
    <!-- Nutzungseinschraenkungen auf "schlechten" Feldwegen und Pfaden -->
    <rule e="way" k="highway" v="feldweg023|feldweg45|path" zoom-min="15">
      <!-- gesperrt = rosa transparentes Overlay -->
      <rule e="way" k="access" v="no|private|agricultural|forestry">
        <rule e="way" k="foot" v="no">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#44FF6FCF" stroke-width="1.8" stroke-linecap="round"/>
        </rule>
        <rule e="way" k="foot" v="~">
          <!-- allgemeines / generelles Nutzungsverbot (durchgehendes Overlay) -->
          <line stroke="#44FF6FCF" stroke-width="1.8" stroke-linecap="round"/>
        </rule>
      </rule>
    </rule>
  </rule>
  <!-- -->
  <!-- -->
  <!-- NODES -->
  <!-- ===== -->
  <!-- -->
  <!-- -->
  <rule e="node" k="*" v="*">
    <!-- -->
    <!-- +++ SHOP +++ -->
    <!-- -->
    <rule e="node" k="shop" v="*">
      <!-- -->
      <!-- Geschaeft (hier keine weitere Differenzierung; s.a. tag_mapping.xml). -->
      <rule e="node" k="*" v="*" zoom-min="16" zoom-max="18">
        <circle r="0.6" scale-radius="true" fill="#00123456" stroke="#0000FF" stroke-width="0.3"/>
      </rule>
      <rule e="node" k="*" v="*" zoom-min="19">
        <circle r="0.2" scale-radius="true" fill="#00123456" stroke="#0000FF" stroke-width="0.15"/>
        <caption k="name" dy="20" font-style="normal" font-size="13" fill="#0000FF" stroke="#ffffff" stroke-width="3.0"/>
      </rule>
    </rule>
  </rule>
</rendertheme>
