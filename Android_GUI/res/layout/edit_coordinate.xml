<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
	android:layout_height="match_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:orientation="horizontal" android:id="@+id/edco_buttonlayout"
		android:layout_width="match_parent" android:layout_alignParentBottom="true"
		android:layout_height="55dip">
		<Button android:layout_weight="1" android:layout_width="match_parent"
			android:text="Button" android:layout_height="match_parent"
			android:id="@+id/edco_ok"></Button>
		<Button android:layout_weight="1" android:layout_width="match_parent"
			android:text="Button" android:layout_height="match_parent"
			android:id="@+id/edco_cancel"></Button>
	</LinearLayout>
	<LinearLayout android:orientation="horizontal" android:id="@+id/edco_titlelayout"
		android:layout_width="match_parent" android:layout_alignParentTop="true"
		android:layout_height="50dip">
		<TextView android:layout_width="match_parent" android:text="TextView"
			android:layout_height="match_parent" android:id="@+id/edco_titleview"></TextView>
	</LinearLayout>
	<LinearLayout android:orientation="horizontal" android:id="@+id/edco_toplayout"
		android:layout_width="match_parent" android:layout_below="@+id/edco_titlelayout"
		android:layout_height="50dip">
		<de.cachebox_test.Custom_Controls.MultiToggleButton
			android:layout_weight="1" android:layout_width="match_parent"
			android:text="Dec" android:layout_height="match_parent" android:id="@+id/edco_dec"></de.cachebox_test.Custom_Controls.MultiToggleButton>
		<de.cachebox_test.Custom_Controls.MultiToggleButton
			android:layout_weight="1" android:layout_width="match_parent"
			android:text="Min" android:layout_height="match_parent" android:id="@+id/edco_min"></de.cachebox_test.Custom_Controls.MultiToggleButton>
		<de.cachebox_test.Custom_Controls.MultiToggleButton
			android:layout_weight="1" android:layout_width="match_parent"
			android:text="Sec" android:layout_height="match_parent" android:id="@+id/edco_sec"></de.cachebox_test.Custom_Controls.MultiToggleButton>
		<de.cachebox_test.Custom_Controls.MultiToggleButton
			android:layout_weight="1" android:layout_width="match_parent"
			android:text="UTM" android:layout_height="match_parent" android:id="@+id/edco_utm"></de.cachebox_test.Custom_Controls.MultiToggleButton>
	</LinearLayout>
	<ScrollView android:id="@+id/scrollView1"
		android:layout_width="match_parent" android:layout_height="match_parent"
		android:layout_above="@+id/edco_buttonlayout" android:layout_below="@+id/edco_toplayout">
		<TableLayout android:layout_width="match_parent"
			android:id="@+id/edco_tableLayout" android:layout_height="match_parent">
			<TableRow android:id="@+id/edco_table_dec"
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<include android:layout_height="wrap_content" android:id="@layout/edit_coordinate_dec"
					layout="@layout/edit_coordinate_dec" android:layout_width="match_parent"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip"
					android:layout_weight="1"></include>

			</TableRow>
			<TableRow android:id="@+id/edco_table_min"
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<include android:layout_height="wrap_content" android:id="@layout/edit_coordinate_min"
					layout="@layout/edit_coordinate_min" android:layout_width="match_parent"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip"
					android:layout_weight="1"></include>

			</TableRow>
			<TableRow android:id="@+id/edco_table_sec"
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<include android:layout_height="wrap_content" android:id="@layout/edit_coordinate_min"
					layout="@layout/edit_coordinate_sec" android:layout_width="match_parent"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip"
					android:layout_weight="1"></include>

			</TableRow>
			<TableRow android:id="@+id/edco_table_utm"
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<include android:layout_height="wrap_content" android:id="@layout/edit_coordinate_utm"
					layout="@layout/edit_coordinate_utm" android:layout_width="match_parent"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip"
					android:layout_weight="1"></include>

			</TableRow>

			<TableRow 
				android:layout_width="wrap_content" android:layout_height="wrap_content">
				<include android:layout_height="wrap_content" android:id="@+id/NumPadLayout"
					layout="@layout/numerik_keypad_layout" android:layout_width="match_parent"
					android:layout_marginLeft="10dip" android:layout_marginRight="10dip"
					android:layout_weight="1"></include>
			</TableRow>

		</TableLayout>
	</ScrollView>



</RelativeLayout>
